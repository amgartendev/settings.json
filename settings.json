{
    "apc.header": {"height": 36},
    "apc.listRow": {"height": 24},
    "apc.electron": {"titleBarStyle": "hiddenInset"},
    "apc.font.family": "JetBrains Mono",
    "apc.stylesheet": {".title-label > h2": "display: none"},
    "explorer.compactFolders": false,
    "terminal.integrated.defaultProfile.windows": "Command Prompt",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontFamily": "JetBrains Mono",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.labelFormat": "short",
    "workbench.activityBar.location": "hidden",
    "workbench.editor.editorActionsLocation": "hidden",
    "workbench.iconTheme": "vscode-icons",
    "workbench.settings.applyToAllProfiles": ["workbench.colorCustomizations"],
    "workbench.colorCustomizations": {"terminal.background": "#00000000"},
    "workbench.editor.empty.hint": "hidden",
    "workbench.colorTheme": "DartPad",
    "workbench.statusBar.visible": false,
    "workbench.layoutControl.enabled": false,
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontSize": 16,
    "editor.lineHeight": 1.8,
    "editor.rulers": [80, 120],
    "editor.renderLineHighlight": "gutter",
    "editor.semanticHighlighting.enabled": false,
    "editor.accessibilitySupport": "off",
    "window.titleBarStyle": "native",
    "window.commandCenter": false,
    "code-runner.runInTerminal": true,
    "security.workspace.trust.untrustedFiles": "open",
}
